{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mgm-interns/team-radio.git"
  },
  "engines": {
    "node": "8.x"
  },
  "lock": false,
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "copy-env:dev": "cp .env.development .env",
    "copy-env:production": "cp .env.production .env",
    "postinstall": "run-s copy-env:production build"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "material-ui": "^1.0.0-beta.26",
    "moment": "^2.19.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-autosuggest": "^9.3.2",
    "react-cropper": "^1.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-flexbox-grid": "^2.0.0",
    "react-icons": "^2.2.7",
    "react-notification-system": "^0.2.16",
    "react-player": "^0.25.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-api-middleware": "^2.0.1",
    "redux-form": "^7.2.0",
    "redux-socket.io": "^1.4.0",
    "redux-thunk": "^2.2.0",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "babel-plugin-root-import": "^5.1.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "react-scripts": "1.0.17"
  }
}
