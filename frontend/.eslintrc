{
  "extends": ["react-app", "prettier"],
  "plugins": ["prettier", "import"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-unused-vars": ["warn"],
    "react/display-name": ["off"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "babel-root-import": {}
    }
  }
}
